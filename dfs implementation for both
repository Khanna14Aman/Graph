//This algorithm willl work for both directed and undirected grph
#include<iostream>
#include<bits/stdc++.h>
#define int long long 
using namespace std;
int component=0;
void dfs(int node,vector<int>&vis,vector<int>adj[])
{
    vis[node]=1;
    cout<<node<<" ";
    for(auto it : adj[node])
    {
        if(!vis[it])
        {
            dfs(it,vis,adj);
        }
    }
}
signed main()
{
    int n,m,i;
    cin>>n>>m;
    vector<int>adj[n+1];
    int u,v; 
    for(i=0;i<m;i++)
    {
        cin>>u>>v;
        adj[u].emplace_back(v);
        adj[v].emplace_back(u);// You can remove this line to make graph directed;
    }
    vector<int>vis(n+1,0);
    for(i=1;i<=n;i++)
    {
        if(!vis[i])
        {
            component++;//  finding component will not work in directed graph
            dfs(i,vis,adj);
        }
    }
    cout<<"\n"<<component;
    return 0;
}
